<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.payroll.headInfo.dataobjects.HeadDepartment" table="head_dept">  
    <composite-id> 
     	<!-- <key-property name="headId" column="head_id"  /> -->
		<key-property name="departmentId" column="dept_id" />  
		<key-property name="headId" column="head_id" />  
    </composite-id>  
    <property name="startDate" column="start_date" not-null="true"/> 
  </class>  
 </hibernate-mapping> 