<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.payroll.headInfo.dataobjects.HeadInfo" table="head_info">  
    <composite-id> 
     	<!-- <key-property name="headId" column="head_id"  /> -->
		<key-property name="departmentId" column="dept_id" />  
		<key-property name="headName" column="head_name" />  
    </composite-id>  
    <property name="headId" column="head_id" not-null="true"/> 
    <property name="description" column="description" not-null="true"/> 
    <property name="status" column="status" not-null="true"/>  
    <property name="rowUpdDate" column="ROW_UPD_DATE" not-null="true"/> 
  </class>  
 </hibernate-mapping> 