<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.payroll.employee.bank.dataobjects.Bank" table="emp_bank">  
    <id name="empId" type="integer" >  
     <column name="empId" sql-type="integer" not-null="true"/>  
    </id>  
    <property name="bankName" column="bank_name" not-null="true"/> 
    <property name="ifscCode" column="ifsc_code" not-null="true"/> 
    <property name="accountNo" column="acct_no" not-null="true"/>  
    <property name="status" column="status" not-null="true"/>  
    <!-- <property name="rowUpdDate" column="ROW_UPD_DATE" not-null="true"/> -->
  </class>  
 </hibernate-mapping> 